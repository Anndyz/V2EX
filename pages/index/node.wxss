.nodes-container{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  margin-top: 10px;
}
.node-title{
  width: 90%;
  /* margin-left: 5%; */
  margin-bottom: 7px;
}

.title-wrapper{
  border-left: 2px solid black;
  padding-left: 5px;
}

.node-list{
  width: 90%;
}

.node-item{
  display: inline-block;
  font-size: 14px;
  padding: 1px 4px;
  border-radius: 5px;
  background-color: #eeeeee;
  margin-right: 15px;
  margin-bottom: 15px;
  vertical-align: middle;
}

.font16{
  font-size: 16px;
}